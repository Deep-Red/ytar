<div class="container col-lg-6 offset-lg-3">
<p class="upload-instructions">List your search terms below; one search per line.</p>

<%= form_tag("/videos/index", method: "get", multipart: true) do %>
  <div class="form-group">
    <%= text_area_tag(:list, "", class: 'form-control') %>
  </div>
    <%= submit_tag "Load", class: 'btn btn-success btn-lg btn-submit col-lg-4 offset-lg-3' %>
<% end %>
</div>
